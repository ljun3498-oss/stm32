{
    "configurations": [
        {
            "name": "Keil ARM",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/User",
                "${workspaceFolder}/Hideware",
                "${workspaceFolder}/Library",
                "${workspaceFolder}/start",
                "${workspaceFolder}/System"
            ],
            "defines": [
                "USE_STDPERIPH_DRIVER",
                "STM32F10X_MD",
                "__CC_ARM",
                "__ARMCC_VERSION=5060000"
            ],
            "compilerPath": "d:/Keil_v5/ARM/ARMCC/bin/armcc.exe",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-arm",
            "browse": {
                "path": [
                    "${workspaceFolder}/User",
                    "${workspaceFolder}/Hideware",
                    "${workspaceFolder}/Library",
                    "${workspaceFolder}/start",
                    "${workspaceFolder}/System"
                ],
                "limitSymbolsToIncludedHeaders": false
            }
        }
    ],
    "version": 4
}
